<%= link_to "Platby", user_payments_path %>
<% form_for(:newuser,@newuser,:url=>{:controller=>"auth",:action => "info"}, :html=>{:class=>"boxRegistration formBox",:multipart => true}) do |f| %>
	<fieldset>
		<div class="step">
	     <h3 class="h">Formulář s osobními údaji</h3>
	  	<div class="Message"><%= f.error_messages %></div>
	  </div>
	</fieldset>	

	<%= render :partial => "auth/user", :locals=>{:f=>f} %>
	<%= submit_tag "Uložit" -%>
<% end %>
