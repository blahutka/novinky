<%= link_to 'Nový uživatel', authadmin_url(:action=>"newuser") %> |
<%= link_to 'Hromadné emaily', mailings_path %>
<% if params[:id] %>
<p>
&gt; Prohlížíte uživatele patřící do skupiny <%= params[:id] %>.<br />
&gt; <%= link_to 'Všichni uživatelé', authadmin_url %>.
</p>
<% end %>

<p style="text-align: center;">
<%= will_paginate @web_users, :inner_window=> 3, :outer_window=> 1,:prev_label   => '&laquo; ' +  'Předchozí',
 	          :next_label   => 'Další' +  ' &raquo;'%>
</p>

<table border="1">
	<tr>
		<td>id</td>
		<td>Login</td>
		<td>Email</td>
		<td>Jméno</td>
		<td>Příjmení</td>
		<td>Upraven</td>
		<td>Domény</td>
		<td>Aktivní?</td>
		<td>Smaž</td>
	</tr>
<%= render :partial => "user", :collection => @web_users %>
</table>

<p style="text-align: center;">
<%= will_paginate @web_users, :inner_window=> 3, :outer_window=> 1,:prev_label   => '&laquo; ' +  'Předchozí',
 	          :next_label   => 'Další' +  ' &raquo;'%>
</p>
