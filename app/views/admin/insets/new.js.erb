$(".recordHeader").remove();
$(".recordPath").remove();
$(".recordEdit").remove();
$("#viewRecord").append("<%= escape_javascript(render(:partial => 'new')) %>");
$("#viewRecord").show();

$("form[id^='new_inset']").ajaxForm(
{
    dataType: 'script',
    beforeSend: function(xhr) {xhr.setRequestHeader("Accept", "text/javascript");},
    complete: function(XMLHttpRequest, textStatus) {
       $(".recordHeader a").click();
    }
    
  }
);