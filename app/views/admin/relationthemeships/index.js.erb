<% if(params[:search_relationthemeships]) %>
$("#TB_ajaxContent").empty();
var cl1 = 'hideSearch';
var cl2 = 'showSearch';
<% else %>
var cl1 = 'showSearch';
var cl2 = 'hideSearch';
<% end %>
$("#TB_ajaxContent").append("<%= escape_javascript(render(:partial => 'header')) %>");
$("#TB_ajaxContent").append("<%= escape_javascript(render(:partial => 'relationthemeships_header')) %>");
$("input[id='cancelSearchRelationthemeships']").removeClass( cl1 );
$("input[id='cancelSearchRelationthemeships']").addClass( cl2 );
$("input[id='cancelSearchRelationthemeships'], input[class='showSearch']").bind("click", function() { $("#TB_ajaxContent").empty(); return getRelationthemeship(); }); 
checkBoxes();