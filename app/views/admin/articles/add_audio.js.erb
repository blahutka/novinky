$(".listFiles.listAudr").append("<%= escape_javascript(render(:partial => 'audio', :object => @audio)) %>");
$("a[id^='audio_<%= @audio.id %>']").bind("click", function() { return removeFileFromArticle( $(this).parent().parent().attr("id").split("_")[1], $(this).attr("id").split("_")[1], 'remove_audio' ) });
