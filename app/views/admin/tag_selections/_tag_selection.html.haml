%tr{ :id => "tagselection_#{tag_selection.id}" }
  %td{ :class => "listItem l1"}
    .cropLine= link_to tag_selection.headline, '#', :title => tag_selection.headline, :onclick => "editRecord($(this), '#{@c_name}' ); return false"
  %td= tag_selection.updated_at.strftime('%d/%m/%Y')
  %td{ :class => 'center' }
    = link_to image_tag('item-edit.gif', :size => "12x11", :alt => "Editovat"), '#', :onclick => "editRecord($(this), '#{@c_name}' ); return false", :title => 'Editovat', :class => "edit_article"
    = link_to image_tag('delete-bin.gif', :size => "12x14", :alt => "Smazat"), '#', :title => 'Smazat' 