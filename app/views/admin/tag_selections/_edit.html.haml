.recordHeader
  %h2= link_to 'Editovat vyber', '#'
.recordPath
.recordEdit

  = error_messages_for :tag_selection

  - form_for @tag_selection, :url => object_url, :html => { :method => :put } do |f|
    %fieldset
      = render :partial => "form", :locals => { :f => f }
      %p= submit_tag "Upravit"