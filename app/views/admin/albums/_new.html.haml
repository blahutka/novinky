.recordHeader
  %h2= link_to 'Nová větev', '#'
.recordPath
.recordEdit  
  - form_for @album, :url => collection_url, :html => { :multipart => true } do |f|
    = f.error_messages
    %fieldset
      = hidden_field_tag :type, 'image'
      = render :partial => "form", :locals => { :f => f }
      %p= submit_tag "Uložit"
    -#= link_to 'Zpět', collection_url