$(".listFiles.listImgr").append("<%= escape_javascript(render(:partial => 'img', :object => @picture)) %>");
$("a[id^='img_<%= @picture.id %>']").bind("click", function() { return removeFileFromArticle( $(this).parent().parent().attr("id").split("_")[1], $(this).attr("id").split("_")[1], 'remove_img' ) });
